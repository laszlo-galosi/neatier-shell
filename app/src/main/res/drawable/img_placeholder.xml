<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/translucent_100" />
        </shape>
    </item>
    <item>
        <rotate
            android:fromDegrees="-45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="@color/colorTextPrimary" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="line">
                <stroke
                    android:width="1dp"
                    android:color="@color/colorTextPrimary" />
            </shape>
        </rotate>
    </item>
</layer-list>

